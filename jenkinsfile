pipeline {
    agent any
    
    stages {
        stage('Execute Node.js Script') {
            steps {
                timeout(time: 1, unit: 'MINUTES') {
                    script {
                        git 'https://github.com/ValtrofTheLeague12/PFE.git'
                        
                        sh 'npm install'
                        
                        sh 'API.js'
                        echo "Sucess !!!"
                    }
                }
            }
        }
    }
}
